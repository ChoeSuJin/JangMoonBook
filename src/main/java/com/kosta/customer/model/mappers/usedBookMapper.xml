<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kosta.customer.model.UsedBookDAO">
	<insert id="writeDao">
		Insert into BOOK_INFO (TITLE,PRICE,AUTHOR,PUBLISHER,TYPE,ISBN,CATEGORY)
		values (#{title}, #{price}, #{author} ,#{publisher} ,'중고도서',#{isbn} ,#{category})
	</insert>
</mapper>
