<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.kosta.book.organ.mOrgan.model.ManageOrganDAO">
	<insert id="ebookReq">
		insert into ebook_reqList values(seq_ebook_reqNo.nextVal, #{isbn}, #{title}, #{com})
	</insert>
	<select id="ebookList" resultType="com.kosta.book.admin.mInventory.model.BookInfoVO">
		select * from book_info where type = 'e_book' order by title
	</select>
	<select id="ebookReqList" resultType="com.kosta.book.organ.mOrgan.model.ManageOrganVO">
		select * from ebook_reqList where com=#{com}
	</select>
</mapper>